# API Backend URL (uses Next.js proxy)
NEXT_PUBLIC_API_URL=/api

# API Timeout in milliseconds (30 seconds)
NEXT_PUBLIC_API_TIMEOUT=30000

# Backend URL for proxy rewrites (used in next.config.mjs)
# In production, set to your backend URL (e.g., https://your-backend.com)
BACKEND_URL=http://localhost:8000
